{
    "name": "pocketmine-code-snippets",
    "displayName": "PocketMine Code Snippets",
    "description": "PocketMine-MP extension for VSCode",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.32.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "php",
                "path": "./snippets/php.json"
            },
            {
                "language": "yaml",
                "path": "./snippets/yaml.json"
            }
        ]
    }
}
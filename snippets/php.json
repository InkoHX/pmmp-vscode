{
  "PocketMinePluginBase": {
    "prefix": "PocketMinePluginBase",
    "body": [
      "namespace $1;",
      "",
      "use pocketmine\\plugin\\PluginBase;",
      "",
      "class $2 extends PluginBase",
      "{",
      "\t$3",
      "}"
    ]
  },
  "PluginBaseOnLoad": {
    "prefix": [
      "public function onLoad",
      "onLoad"
    ],
    "description": "Called when the plugin is loaded, before calling onEnable()",
    "body": [
      "public function onLoad()",
      "{",
      "\t${Code}",
      "}"
    ]
  },
  "PluginBaseOnEnable": {
    "prefix": [
      "public function onEnabled",
      "onEnabled"
    ],
    "description": "Called when the plugin is enabled",
    "body": [
      "public function onEnable()",
      "{",
      "\t${Code}",
      "}"
    ]
  },
  "PluginBaseOnDisable": {
    "prefix": [
      "public function onDisable",
      "onDisable"
    ],
    "description": "Called when the plugin is disabled",
    "body": [
      "public function onDisable()",
      "{",
      "\t${Code}",
      "}"
    ]
  }
}